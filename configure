     yes) ENABLE_DYNAMIC_LINKING=yes ;;
     no) ENABLE_DYNAMIC_LINKING=no ;;
     *) as_fn_error $? "bad value $enableval for --enable-dl" "$LINENO" 5 ;;
   esac
